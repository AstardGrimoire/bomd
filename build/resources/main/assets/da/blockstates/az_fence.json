{
  "multipart": [
    {   "apply": { "model": "da:bars_post" }},
    {   "when": { "north": true },
      "apply": { "model": "da:bars_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "da:bars_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "da:bars_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "da:bars_side_alt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "da:bars_noside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "da:bars_noside_alt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "da:bars_noside_alt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "da:bars_noside", "y": 270 }
    }
  ]
}